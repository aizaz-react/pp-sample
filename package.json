{
  "name": "pp-client",
  "version": "0.1.0",
  "private": true,
  "proxy": "https://api.promptprivacy.com:5000",
  "scripts": {
    "dev": "ncp ./.env.test ./.env.local && next dev",
    "build": "next build",
    "build:dev": "ncp ./.env.dev ./.env.local && yarn build",
    "build:uat": "ncp ./.env.uat ./.env.local && yarn build",
    "build:prod": "ncp ./.env.prod ./.env.local && yarn build",
    "deploy:dev": "yarn && yarn build:dev && gcloud app deploy dev.yaml --quiet",
    "deploy:uat": "yarn && yarn build:uat && gcloud app deploy uat.yaml --quiet",
    "deploy:prod": "yarn && yarn build:prod && gcloud app deploy prod.yaml --quiet",
    "deploy:all": "yarn deploy:dev && yarn deploy:uat yarn deploy:prod",
    "start": "next start -p 8080",
    "lint": "next lint",
    "deploy": "yarn && yarn build && gcloud app deploy --quiet",
    "testBuild": "clear && yarn build && yarn start",
    "testDev": "clear && yarn dev"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.3",
    "@mui/x-data-grid": "^6.6.0",
    "@react-oauth/google": "^0.11.0",
    "@stripe/react-stripe-js": "^2.2.0",
    "@stripe/stripe-js": "^2.1.2",
    "@tanstack/react-query": "^4.29.12",
    "apexcharts": "^3.41.0",
    "axios": "^1.4.0",
    "eslint": "8.42.0",
    "eslint-config-next": "^13.5.4",
    "formik": "^2.4.1",
    "highlight.js": "^11.8.0",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.4",
    "ncp": "^2.0.0",
    "next": "^13.5.4",
    "next-auth": "^4.23.1",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.1",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-flatpickr": "^3.10.13",
    "react-toastify": "^9.1.3",
    "sass": "^1.66.1",
    "stripe": "^13.5.0",
    "validator": "^13.11.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^4.29.25",
    "env-cmd": "^10.1.0",
    "typescript": "5.2.2"
  }
}
